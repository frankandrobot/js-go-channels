{
  "name": "js-go-channels",
  "version": "0.0.11",
  "description": "golang channels for JS",
  "homepage": "https://github.com/frankandrobot/js-go-channels",
  "repository": "frankandrobot/js-go-channels",
  "main": "dist/legacy-browser/index.js",
  "module": "dist/legacy-webpack/index.js",
  "jsnext:main": "dist/legacy-webpack/index.js",
  "scripts": {
    "test": "tape -r ./babeltest.config 'tests/**/*.js' | tap-notify | tap-diff",
    "test:simple": "tape -r ./babeltest.config -r babel-polyfill 'tests/**/*.js'",
    "prerelease": "npm run doc:src && npm run doc:api && npm run dist && git add doc dist && git commit -m \"[dist]\"",
    "clean": "rimraf dist && mkdirp dist/legacy-node && mkdirp dist/legacy-browser",
    "build:legacy-node": "BABEL_ENV=legacy-node babel src --source-maps --no-comments --out-file dist/legacy-node/index.js",
    "build:legacy-browser": "BABEL_ENV=legacy-browser babel src --source-maps --no-comments --out-file dist/legacy-browser/index.js",
    "build:legacy-webpack": "BABEL_ENV=legacy-webpack-es babel src --source-maps --no-comments --out-dir dist/legacy-webpack",
    "build": "npm run clean && run-p build:**",
    "release:major": "npm version major && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:patch": "npm version patch && git push --follow-tags",
    "dist": "rollup --config"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "core-js": "^2.5.0",
    "dependo": "^0.1.6",
    "docco": "^0.7.0",
    "documentation": "^4.0.0-rc.1",
    "eslint": "^3.19.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.49.1",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "tap-diff": "^0.1.1",
    "tap-notify": "^1.0.0",
    "tape": "^4.6.3",
    "timed-tape": "^0.1.1"
  }
}
